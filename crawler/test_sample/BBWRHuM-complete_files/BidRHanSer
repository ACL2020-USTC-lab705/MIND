    var script = document.createElement("script");
    script.onload = onUnitLoaderLoad;
    // script.src = "//vidstat.taboola.com/vpaid/units/26_6_3/creatives/creative_js.js";
    
	var onUnitLoaderLoadStr = onUnitLoaderLoad.toString();
    var unitType = /(?:'|")(?:unitType)(?:'|"),\s*[\"|\']([^"]+)[\"|\']/.exec(onUnitLoaderLoad);
    var isMobile = /(?:'|")(?:isMobile)(?:'|"),\s*([^"]+)\)/.exec(onUnitLoaderLoad);
    var liteUnitFile;

    if (unitType && unitType[1]) {
        var unitName = unitType[1];
        if (unitName === 'FEED_MANAGER') {
            liteUnitFile = 'UnitFeedManager';
        } else if (unitName === 'WIDGET_ITEM') {
            liteUnitFile = 'UnitWidgetItem';
        } else if (unitName === 'INLINE_INSTREAM') {
            liteUnitFile = 'UnitInline';
        } else if (unitName === 'SLIDER_INSTREAM') {
            liteUnitFile = 'UnitSlider';
        } else if (unitName === 'EXPANDABLE') {
            liteUnitFile = 'UnitExpandable';
        } else if (unitName === 'MSN_WIDGET') {
            liteUnitFile = 'UnitMSNWidget';
        }
    }

    if (liteUnitFile) {
        liteUnitFile += (isMobile ? 'Mobile' : 'Desktop') + '.min.js';
        script.src = "//vidstat.taboola.com/lite-unit/1.7.1/" + liteUnitFile;
    } else {
        script.src = "//vidstat.taboola.com/vpaid/units/26_6_3/creatives/creative_js.js";
    }
    
    document.getElementsByTagName('head')[0].appendChild(script);
    })(); //
